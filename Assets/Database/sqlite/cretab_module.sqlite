CREATE TABLE sc_module (
	id integer NOT NULL PRIMARY KEY AUTOINCREMENT , 
	name varchar(255) NOT NULL , 
	visual varchar(4000) , 
	order integer NOT NULL , 
	session_id integer NOT NULL 
);



